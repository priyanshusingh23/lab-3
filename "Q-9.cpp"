 0;#include <iostream>
using namespace std;

int main( )  {

char ch;

cout << "enter a character, specialcharacter, alphabet";
cin>> ch;

if (ch >='A'&& ch<='Z' || ch>='a'&& ch<='z')
{cout<< "ch is alphabet";}

 else if ( ch>='0' && ch<='9')
{ cout<< "ch is character"; }

else {cout<< "ch is special character";}

	return
}
